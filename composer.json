{
    "name": "10up/primarycategoryplugin",
    "description": "WordPress plugin that allows publishers to designate a primary category for posts.",
    "type": "wordpress-plugin",
    "require": {
        "php": "^8.1",
        "composer/composer": "^2.7"
    },
    "require-dev": {
        "10up/wp_mock": "*",
        "phpunit/phpunit": "^9.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "PrimaryCategoryPlugin\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jenny Martinez",
            "email": "jenny.eileen.mtz@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
